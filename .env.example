# Portal Guru Environment Variables
# Copy this file to .env and fill in your values

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# AI Services (Optional)
# Recommended: use a server-side proxy to avoid exposing keys in client bundles.
VITE_OPENROUTER_PROXY_URL=https://your-domain.vercel.app/api/openrouter
# Local-only fallback (avoid using in production)
VITE_OPENROUTER_API_KEY=your-openrouter-key
OPENROUTER_API_KEY=your-openrouter-key
# Optional origin allowlist for the proxy
OPENROUTER_ALLOWED_ORIGIN=https://your-domain.vercel.app

# Sentry Error Tracking (Optional)
VITE_SENTRY_DSN=https://your-sentry-dsn

# Feature Flags (Optional)
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_AI_FEATURES=false

# Environment
VITE_APP_ENV=development
